{
  "name": "vue-chrome-hmr",
  "description": "Demo project to get HRM working in a Chrome extension",
  "version": "0.1.0",
  "scripts": {
    "dev": "vue-cli-service serve",
    "watch": "vue-cli-service build --watch",
    "watch:bg": "webpack --config background.config.js -d --watch",
    "build": "vue-cli-service build && npm run build:bg",
    "build:bg": "webpack --config background.config.js"
  },
  "dependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-cli": "^3.2.3"
  }
}
